package InnerClass.Anynomous;

@FunctionalInterface
interface Vehicle{
	int getNumber();
//	int getMilage();
}
public class AnynomousDemo  {

	public static void main(String[] args) {
		
          Vehicle v=new Vehicle() {
        	  public int getNumber() {
        		  return 1234;
        	  }
          };
	}

}
